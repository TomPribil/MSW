    "import psutil\n",
    "import random\n",
    "\n",
    "random.seed()\n",
    "\n",
    "seed = int(psutil.cpu_percent(0) * psutil.virtual_memory()[1])\n",
    "random.seed(seed)\n",
    "gen_random = random.random()\n",
    "\n",
    "print(f\"Vygenerovaný seed: {seed}\")\n",
    "print(f\"Vygenerované náhodné číslo: {gen_random}\")"
   ]
